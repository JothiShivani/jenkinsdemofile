pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
            
        }
         stage('Build') {
            steps {
                echo 'Hello Build'
            }
            
        }
         stage('deploy') {
            steps {
                echo 'Hello deploy'
            }
            
        }
      stage('Testing') {
            steps {
                echo 'Hello Testing'
            }
            
        }
      stage('Release') {
            steps {
                echo 'Hello Release'
            }
            
        }
    }
}
